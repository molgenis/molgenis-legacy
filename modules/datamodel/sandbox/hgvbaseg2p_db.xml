        <field name="Attribute" default=""/>
        <field name="PhenotypeValueDetail" label="Phenotype value detail" />
        <field name="UnitOfMeasurement" label="Unit of measurement" nillable="true"/>
        <field name="Qualifier" nillable="true"/>
        <unique fields="Attribute,PhenotypeValueID"/>
    </entity>
	
	
    <entity name="Researcher" xref_label="ShortName">
