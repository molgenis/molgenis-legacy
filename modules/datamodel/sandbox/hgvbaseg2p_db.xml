        <field name="FullName" label="Full name" nillable="true"/>
        <field name="Department" nillable="true"/>
        <field name="Institution"/>
        <field name="Address" nillable="true"/>
        <field name="Phone" nillable="true"/>
        <field name="Fax" nillable="true"/>
        <field name="Email" label="E-mail" nillable="true"/>
        <field name="WWW" nillable="true"/>
        <unique fields="ResearcherID,ShortName"/>
    </entity>
	
	
    <entity name="Resultset" xref_label="Identifier" decorator="decorators.ResultsetDecorator">
        <field name="ResultsetID" label="Result set ID" type="autoid" hidden="true" default="0" />
        <!--field name="Identifier" label="Result set identifier" nillable="true" unique="true"/-->
        <field name="Identifier" label="Result set identifier" unique="true"/>
