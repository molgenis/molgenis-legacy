    <entity name="CitationCrossref">
        <field name="CitationCrossrefID" label="Citation crossref ID" type="autoid"/>
        <field name="CitationID" label="Citation ID" type="xref" default="0" xref_field="Citation.CitationID"/>
        <field name="CrossrefID" label="Crossref ID" type="xref" default="0" xref_field="Crossref.CrossrefID"/>
