    <!-- table DiseaseCategory has been replaced with table PhenotypeAnnotation -->
    <entity name="DiseaseCategory">
        <field name="DiseaseCategoryID" label="Disease category ID" type="autoid" hidden="true" default="0" />
        <field name="ParentCategoryID" label="Parent category ID" type="xref" nillable="true" description="PK for parent category, if this is not a root term" xref_field="DiseaseCategory.DiseaseCategoryID"/>
